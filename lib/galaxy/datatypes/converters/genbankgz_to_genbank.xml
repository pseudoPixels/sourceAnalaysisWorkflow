<tool id="CONVERTER_genbankgz_to_genbank" name="Convert genbank.gz files to genbank" version="1.0.0" hidden="true">
  <command>gzip -dcf '$input1' > '$output1'</command>
  <inputs>
    <param format="genbank.gz" name="input1" type="data" label="Choose GenBank file"/>
  </inputs>
  <outputs>
    <data format="genbank" name="output1"/>
  </outputs>
  <help>
  </help>
</tool>
